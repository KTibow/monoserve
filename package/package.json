{
  "name": "monoserve",
  "version": "2.2.1",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/KTibow/monoserve/tree/main/package#readme",
  "bugs": {
    "url": "https://github.com/KTibow/monoserve/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KTibow/monoserve.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.js"
    }
  },
  "bin": {
    "monoserve-gen-env": "./dist/gen-env-cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "tsdown && node env-support.ts",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "rolldown": "1.0.0-beta.34"
  },
  "devDependencies": {
    "@types/deno": "^2.3.0",
    "@types/node": "^22.15.17",
    "devalue": "^5.3.2",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vite": "^7.1.5"
  },
  "peerDependencies": {
    "devalue": "^5.3.2",
    "vite": "^7.1.5"
  }
}
